{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "Littlebook",
	"version": "0.1.0",
	"identifier": "app.littlebook.desktop",
	"build": {
		"devUrl": "http://localhost:1111",
		"frontendDist": "../../app/dist"
	},
	"app": {
		"windows": [
			{
				"title": "Littlebook",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
