{
	"name": "@littlebook/tldraw",
	"description": "a littlebook content plugin for tldraw support",
	"version": "1.0.0",
	"type": "module",
	"main": "output/index.js",
	"littlebook": {
		"importers": [
			{
				"type": "file",
				"displayName": "tldraw .tldr file",
				"extensions": [
					"tldr"
				],
				"mimeTypes": [
					"application/json"
				]
			},
			{
				"type": "file",
				"extensions": [
					"ocif"
				],
				"mimeTypes": [
					"application/json"
				]
			},
			{
				"type": "url",
				"displayName": "tldraw URL",
				"urls": [
					"https://tldraw.com/*"
				]
			}
		],
		"exporters": [
			{
				"type": "file",
				"extension": "tldr"
			},
			{
				"type": "file",
				"extension": "ocif"
			}
		],
		"editors": [
			{
				"element": "littlebook-tldraw",
				"contentTypes": [
					"com.tldraw.file"
				],
				"displayName": "tldraw editor"
			}
		]
	},
	"scripts": {
		"mk": "NODE_ENV=production node mk",
		"w": "node mk/w",
		"dev": "NODE_ENV=development node mk/w",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "chee",
	"license": "AGPL-3.0+",
	"devDependencies": {
		"@automerge/automerge": "^2.2.2",
		"esbuild": "^0.21.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tldraw": "^2.3.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@tldraw/assets": "^2.3.0",
		"@tldraw/editor": "^2.3.0",
		"@tldraw/tldraw": "^2.3.0",
		"@tldraw/tlschema": "^2.3.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/throttle-debounce": "^5.0.2",
		"throttle-debounce": "^5.0.0",
		"tldraw-vscode": "^1.28.2"
	},
	"peerDependencies": {
		"@automerge/automerge": "^2.2.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tldraw": "^2.3.0"
	}
}
